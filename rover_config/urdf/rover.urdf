        <parent link="arm3_link3"/>
        <child link="grip_connector"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="left_grip_joint" type="revolute">
        <origin xyz="0.07 -0.05 -0.1" rpy="0.0 0.0 0.0"/>
        <parent link="grip_connector"/>
        <child link="left_grip"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="right_grip_joint" type="revolute">
        <origin xyz="-0.07 -0.05 -0.1" rpy="0.0 0.0 0.0"/>
        <parent link="grip_connector"/>
        <child link="right_grip"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>




</robot>
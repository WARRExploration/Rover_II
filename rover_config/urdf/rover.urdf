        <child link="wheel_back_left"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="wheel_back_right_joint" type="revolute">
        <origin xyz="-0.2 0.7 0" rpy="0.0 0.0 0.0"/>
        <parent link="right_rocker"/>
        <child link="wheel_back_right"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>


    <joint name="arm_link_joint" type="revolute">
        <origin xyz="0 -0.5 0.4" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="arm_link"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

   

    <joint name="arm2_link2_joint" type="revolute">
        <origin xyz="0 0 0.70" rpy="0.0 0.0 0.0"/>
        <parent link="arm_link"/>
        <child link="arm2_link2"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>


    <joint name="arm3_link3_joint" type="revolute">
        <origin xyz="0 -0.55 -0.13" rpy="0.0 0.0 0.0"/>

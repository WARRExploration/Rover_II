<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="rover_model">

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.7 1.4 0.4"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <material name="Brown">
             <color rgba="${112/255} ${51/255} ${10/255} 1.0"/>
        </material>
        </visual>
    </link>

    <link name="left_rocker">
        <visual>
            <geometry>
                <box size="0.2 1.4 0.2"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0.1 0 0.1"/>
            <material name="Grey">
         <color rgba="${203/255} ${200/255} ${191/255} 1.0"/>
    </material>
        </visual>
    </link>

    <link name="right_rocker">
        <visual>
            <geometry>
             <box size="0.2 1.4 0.2"/>   
                <box size="0.2 1.4 0.2"/>
             <box size="0.2 1.4 0.2"/>   
            </geometry>
            <origin rpy="0 0 0" xyz="-0.1 0 0.1"/>
            <material name="Grey">
         <color rgba="${203/255} ${200/255} ${191/255} 1.0"/>
    </material>
        </visual>
    </link>

    <link name="wheel_front_left">
        <visual>
            <geometry>
                <cylinder radius="0.15" length="0.2"/>
            </geometry>
            <origin rpy="0 1.570796 0" xyz="0.1 0 0"/>
             <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
        </visual>
    </link>

    <link name="wheel_front_right">
        <visual>
            <geometry>
                <cylinder radius="0.15" length="0.2"/>
            </geometry>
            <origin rpy="0 1.570796 0" xyz="-0.1 0 0"/>
            <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
        </visual>
    </link>

    <link name="wheel_back_left">
        <visual>
            <geometry>
                <cylinder radius="0.15" length="0.2"/>
            </geometry>
            <origin rpy="0 1.570796 0" xyz="0.1 0 0"/>
            <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
        </visual>
    </link>

    <link name="wheel_back_right">
        <visual>
            <geometry>
                <cylinder radius="0.15" length="0.2"/>
            </geometry>
            <origin rpy="0 1.570796 0" xyz="-0.1 0 0"/>
             <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
        </visual>
    </link>

     <link name="arm_link">
        <visual>
            <geometry>
                <box size="0.1 0.1 1.0"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0.3"/>
            <material name="Green">
             <color rgba="${111/255} ${155/255} ${137/255} 1.0"/>
        </material>
        </visual>
    </link>


    <link name="arm2_link2">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.7"/>
            </geometry>
            <origin rpy="-1.3 0 0" xyz="0 -0.3 -0.05"/>
             <material name="Green">
             <color rgba="${111/255} ${155/255} ${137/255} 1.0"/>
        </material>
        </visual>
    </link>


    <link name="arm3_link3">
        <visual>
            <geometry>
                <cylinder radius="0.05" length="0.4"/>
            </geometry>
            <origin rpy="-0.5 0 0" xyz="0 -0.1 -0.15"/>
             <material name="Green">
             <color rgba="${111/255} ${155/255} ${137/255} 1.0"/>
        </material>
        </visual>
    </link>

    <link name="grip_connector">
        <visual>
            <geometry>
            <cylinder radius="0.05" length="0.25"/>
            </geometry>
            <origin rpy="0 1.570796 0" xyz="0 0 0"/>
            <material name="Purple">
             <color rgba="${78/255} ${88/255} ${188/255} 1.0"/>
        </material>
             </visual>
        </link>

        <link name="left_grip">
        <visual>
            <geometry>
            <cylinder radius="0.03" length="0.25"/>
            </geometry>
            <origin rpy="0 0.2 0" xyz="0 0 0"/>
            <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
             </visual>
        </link>

         <link name="right_grip">
        <visual>
            <geometry>
            <cylinder radius="0.03" length="0.25"/>
            </geometry>
            <origin rpy="0 -0.2 0" xyz="0 0 0"/>
            <material name="black">
             <color rgba="${12/255} ${12/255} ${12/255} 1.0"/>
        </material>
             </visual>
        </link>
      
  
    <joint name="left_rocker_joint" type="revolute">
        <origin xyz="0.35 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_rocker"/>
        <axis xyz="1 0 0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <joint name="right_rocker_joint" type="revolute">
        <origin xyz="-0.35 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="right_rocker"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="wheel_front_left_joint" type="revolute">
        <origin xyz="0.2 -0.7 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="left_rocker"/>
        <child link="wheel_front_left"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="wheel_front_right_joint" type="revolute">
        <origin xyz="-0.2 -0.7 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="right_rocker"/>
        <child link="wheel_front_right"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="0" effort="0" velocity="0"/>
    </joint>

    <joint name="wheel_back_left_joint" type="revolute">
        <origin xyz="0.2 0.7 0.0" rpy="0.0 0.0 0.0"/>
